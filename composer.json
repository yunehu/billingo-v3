{
	"name": "yunehu/billingo-v3",
	"description": "Billingo API v3",
	"keywords": [
		"billingo",
		"php",
		"api",
		"v3"
	],
	"homepage": "https://www.billingo.hu",
	"license": "proprietary",
	"authors": [
		{
			"name": "Billingo",
			"homepage": "https://www.billingo.hu"
		}
	],
	"require": {
		"php": ">=5.5",
		"ext-curl": "*",
		"ext-json": "*",
		"ext-mbstring": "*",
		"guzzlehttp/guzzle": "^6.2"
	},
	"require-dev": {
		"phpunit/phpunit": "^4.8",
		"squizlabs/php_codesniffer": "~2.6",
		"friendsofphp/php-cs-fixer": "~1.12"
	},
	"autoload": {
		"psr-4": {
			"Billingo\\": "lib/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Billingo\\": "test/"
		}
	}
}
